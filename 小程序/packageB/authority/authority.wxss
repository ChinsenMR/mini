#vipList .content {
  box-sizing: border-box;
  padding: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
}
#vipList .content .main_wrap {
  width: 49%;
  border-radius: 15rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
  position: relative;
}
#vipList .content .main_wrap image {
  width: 100%;
  height: 320rpx;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
}
#vipList .content .main_wrap .main {
  padding: 20rpx 10rpx;
  height: 180rpx;
}
#vipList .content .main_wrap .main .title {
  font-size: 28rpx;
  color: #333;
  height: 80rpx;
}
#vipList .content .main_wrap .main .price {
  margin: 10rpx 0;
}
#vipList .content .main_wrap .main .price text {
  font-size: 20rpx;
  color: #ea5464;
}
#vipList .content .main_wrap .main .price text:nth-child(2) {
  font-size: 28rpx;
  font-weight: 600;
}
#vipList .content .main_wrap .main .member {
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  font-size: 24rpx;
  color: #fff;
  width: 100rpx;
  height: 40rpx;
  text-align: center;
  line-height: 40rpx;
  border-radius: 20rpx;
  background-color: #b61e35;
}
#vipList .no_data {
  font-size: 30rpx;
  color: #999;
  margin: 40rpx 0;
  text-align: center;
}
.add_active {
  overflow: hidden;
  height: 100vh;
}
.win_wrap {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  background-color: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 15rpx;
  width: 90%;
  max-height: 600rpx;
  z-index: 80;
  overflow: hidden;
}
.win_wrap .title {
  font-size: 30rpx;
  color: #333;
  margin: 15rpx 0;
}
.win_wrap .win_level {
  height: 380rpx;
  overflow: auto;
  padding: 0 20rpx;
}
.win_wrap .win_level .content_wrap {
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
}
.win_wrap .win_level .content_wrap .info {
  font-size: 28rpx;
  color: #333;
  font-weight: 600;
  width: 140rpx;
}
.win_wrap .win_level .content_wrap .input_wrap {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 20rpx;
  border: 1rpx solid #007acc;
  height: 60rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
  position: relative;
}
.win_wrap .win_level .content_wrap .input_wrap .title_input {
  position: absolute;
  right: -999999999999999;
  bottom: -9999999;
  display: none;
  background-color: pink;
  z-index: -9999;
  width: 0;
  height: 0;
}
.win_wrap .win_level .content_wrap .input_wrap text {
  font-size: 26rpx;
  color: #666;
}
.win_wrap .win_level .content_wrap .input_wrap .seach_wrap {
  position: absolute;
  bottom: -120rpx;
  left: 20rpx;
  width: 70%;
  line-height: 60rpx;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  border: 1rpx solid #eee;
  border-top: 0;
  z-index: 999;
}
.win_wrap .win_level .content_wrap .input_wrap .seach_wrap text {
  border-bottom: 1rpx solid #eee;
}
.win_wrap .win_level .content_wrap .input_wrap .active {
  animation: round 2s linear;
}
@keyframes round {
  0% {
    transform: rotate(90deg);
  }
  50% {
    transform: rotate(-180deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
.win_wrap .win_level .content_wrap .input_wrap .sjx_btn {
  width: 18rpx;
  height: 16rpx;
  transform: rotate(90deg);
  position: absolute;
  top: 20rpx;
  right: 30rpx;
}
.win_wrap .win_level .content_wrap input {
  flex: 1;
  border: 1rpx solid #007acc;
  border-radius: 10rpx;
  height: 60rpx;
  padding: 0 15rpx;
}
.win_wrap .btn_wrap {
  position: absolute;
  right: 50rpx;
  bottom: 20rpx;
  display: flex;
  align-items: center;
}
.win_wrap .btn_wrap text {
  font-size: 28rpx;
  color: #fff;
  padding: 15rpx 30rpx;
  background-color: #0091ea;
  border-radius: 10rpx;
  border: 1rpx solid #57b6f1;
}
.win_wrap .btn_wrap button {
  width: 118rpx;
  height: 68rpx;
  font-size: 28rpx;
  color: #fff;
  background-color: #0091ea;
  border-radius: 10rpx;
  border: 1rpx solid #57b6f1;
}
.win_wrap .btn_wrap text:nth-child(2) {
  margin-left: 40rpx;
  color: #333;
  background-color: #f8f8f8;
  border: 1rpx solid #e5e5e5;
}

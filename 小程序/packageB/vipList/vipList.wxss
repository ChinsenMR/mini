#vipList .nav_wrap {
  background: linear-gradient(115deg, #e7643e 0%, #e85448 38%, #e43e51 100%);
  box-sizing: border-box;
  padding: 0 20rpx;
}
#vipList .nav_wrap .scroll_nav {
  white-space: nowrap;
}
#vipList .nav_wrap .scroll_nav .nav {
  display: inline-block;
  padding: 20rpx 30rpx;
  font-size: 28rpx;
  color: #fff;
  position: relative;
}
#vipList .nav_wrap .scroll_nav .active::before {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 5rpx;
  width: 50rpx;
  height: 6rpx;
  background-color: #fff;
  border-radius: 3rpx;
}
#vipList .content {
  box-sizing: border-box;
  padding: 20rpx 20rpx 120rpx 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  flex-wrap: wrap;
}
#vipList .content .main_wrap {
  width: 49%;
  border-radius: 15rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
#vipList .content .main_wrap image {
  width: 100%;
  height: 320rpx;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
}
#vipList .content .main_wrap .main {
  padding: 20rpx 10rpx;
  height: 180rpx;
}
#vipList .content .main_wrap .main .title {
  font-size: 28rpx;
  color: #333;
  height: 80rpx;
}
#vipList .content .main_wrap .main .price {
  margin: 10rpx 0;
}
#vipList .content .main_wrap .main .price text {
  font-size: 20rpx;
  color: #ea5464;
}
#vipList .content .main_wrap .main .price text:nth-child(2) {
  font-size: 28rpx;
  font-weight: 600;
}
#vipList .content .main_wrap .main .member {
  background: url('https://img.hmeshop.cn/hmeshop_jxy/images/icon_hy@3x.png') no-repeat;
  background-size: 102rpx 40rpx;
  width: 85%;
  height: 40rpx;
  border-radius: 20rpx;
  background-color: #efefef;
  box-sizing: border-box;
  padding-left: 55rpx;
  display: flex;
  align-items: center;
}
#vipList .content .main_wrap .main .member text:nth-child(1) {
  font-size: 20rpx;
  color: #FFE1AE;
  font-weight: 600;
}
#vipList .content .main_wrap .main .member text:nth-child(2) {
  font-size: 22rpx;
  color: #333;
  margin-left: 30rpx;
}

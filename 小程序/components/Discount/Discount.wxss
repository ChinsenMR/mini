.newpeopleitem {
  width: 317rpx;
  height: 474rpx;
  margin-right: 11rpx;
  margin-top: 14rpx;
  display: inline-block;
  vertical-align: top;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  padding: 10rpx;
}

.newpeopleitemimgbox {
  width: 314rpx;
  height: 323rpx;
  position: relative;
  border-radius: 8rpx 8rpx 0px 0px;
  /* background: skyblue; */
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

.newpeopleitemimgbox image {
  width: 100%;
  height: 100%;
}

.peoplenamebox {
  width: 314rpx;
  height: 88rpx;
  position: absolute;
  bottom: 0rpx;
}

.peoplenamebox .productname {
  margin-left: 20rpx;
  position: absolute;
  bottom: 4rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: rgba(254, 254, 254, 1);
  font-size: 25rpx;
}

.productpricebox {
  height: 38rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  position: absolute;
  bottom: 24rpx;
  right: 18rpx;
}

.productpricebox .producttitle {
  font-size: 12rpx;
  color: rgba(254, 254, 254, 1);
}

.productpricebox .productprice {
  height: 16rpx;
  font-size: 12rpx;
  margin-top: 6rpx;
  color: rgba(254, 254, 254, 1);
}

.productpricebox .yuanjia {
  font-size: 12rpx;
  color: rgba(254, 254, 254, 1);
}

.peoplenamebox image {
  width: 100%;
  height: 100%;
}

.newpeopleitem .peoplename {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 298rpx;
  font-size: 28rpx;
  line-height: 35rpx;
  margin: 10rpx 19rpx 0 8rpx;
  color: rgba(51, 51, 51, 1);
}

.newpeopleitem .peoplepricebox {
  margin-top: 7rpx;
  margin-left: 20rpx;
  align-items: center;
  position: relative;
}

.peoplepricebox .peoplepriceacting {
  /* height: 25rpx; */
  display: flex;
  align-items: flex-end;
  font-size: 32rpx;
  color: rgba(255, 51, 51, 1);
  box-sizing: border-box;
  margin-top: 20rpx;

}

.peoplepriceacting text:nth-of-type(1) {
  /* align-self: flex-end; */
  font-size: 22rpx;
}

.peoplepriceacting text:nth-of-type(2) {
  /* align-self: flex-end; */
  font-size: 32rpx;
  line-height: 32rpx;
}

.peoplepriceacting .new {
  display: inline-block;
  width: 91rpx;
  height: 30rpx;
  border: 1rpx solid rgba(255, 255, 255, 1);
  background: linear-gradient(83deg, rgba(255, 68, 68, 1) 0%, rgba(255, 51, 102, 1) 100%);
  border-radius: 15rpx 15rpx 15rpx 0rpx;
  color: #fff;
  font-size: 14rpx;
  text-align: center;
  line-height: 30rpx;
  align-self: flex-end;
  margin-left: 16rpx;
}

.peoplepricebox .peoplepricenoacting {
  /* width: 86rpx; */
  height: 16rpx;
  margin-top: 9rpx;
  font-size: 18rpx;
  color: rgba(102, 102, 102, 1);
  text-decoration: line-through;
}

.peoplepricenoacting text,
.yuanjia text {
  text-decoration: line-through;
}

.more {
  width: 50rpx;
  height: 50rpx;
  margin-left: 96rpx;
  background: rgba(227, 178, 125, 1);
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: -15rpx;
  right: 13rpx;
}

.more text {
  width: 22rpx;
  height: 2rpx;
  position: absolute;
  background: rgba(255, 255, 255, 1);
}

.more text:nth-of-type(1) {
  transform: rotate(90deg);
}

.limitTime {
  color: #f33;
  font-size: 20rpx;
  padding-left: 19rpx;
  margin: 10rpx 0;
  font-weight: 500;
}

.limitTime text:nth-of-type(1) {
  margin-right: 5rpx;
}

.limitTime text:nth-of-type(3),
.limitTime text:nth-of-type(5) {
  margin: 0 5rpx;
}

.no_data{
  margin:200rpx auto;
  text-align: center;
}
.hint{
  padding: 30rpx 0;
  text-align: center;
  font-size: 32rpx;
  color:#999;
}
.myTeam {
  height: 100%;
}
.myTeam .content {
  height: 100%;
}
.myTeam .content .data_wrap {
  font-size: 24rpx;
  color: #666666;
  display: flex;
  align-items: center;
}
.myTeam .content .data_wrap text {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx 0;
}
.myTeam .content .content-search {
  height: 88rpx;
  padding: 0 28rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #F4F4F4;
}
.myTeam .content .content-search .search-item {
  width: 253rpx;
  height: 53rpx;
  border-radius: 27rpx;
  background-color: rgba(204, 204, 204, 0.15);
  border: 1rpx solid rgba(102, 102, 102, 0.15);
  margin-right: 28rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
}
.myTeam .content .content-search .search-item image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}
.myTeam .content .content-search .search-item input {
  font-size: 20rpx;
}
.myTeam .content .content-search .content-search-R {
  color: #333333;
  font-size: 19rpx;
}
.myTeam .content .content-search .content-search-R image {
  width: 16rpx;
  margin-left: 5rpx;
}
.myTeam .content-list {
  margin-top: 20rpx;
  background-color: #fff;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.myTeam .content-list-top {
  display: flex;
  align-items: center;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #eee;
}
.myTeam .content-list-top-img_wrap {
  position: relative;
  margin-right: 20rpx;
}
.myTeam .content-list-top-img_wrap image {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
}
.myTeam .content-list-top-img_wrap-level {
  position: absolute;
  left: -3%;
  bottom: -20rpx;
  font-size: 16rpx;
  color: #fff;
  width: 100rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  background: linear-gradient(64deg, #333333 0%, #999999 100%);
  border-radius: 20rpx;
  max-width: 150rpx;
}
.myTeam .content-list-top-info_wrap {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 24rpx;
  color: #999999;
}
.myTeam .content-list-top-info_wrap text:nth-child(1) {
  font-size: 28rpx;
  color: #333333;
  font-weight: 600;
}
.myTeam .content-list-top-time_wrap {
  width: 238rpx;
  font-size: 22rpx;
  color: #999999;
  height: 88rpx;
}
.myTeam .content-list-bottom-agency_wrap {
  font-size: 24rpx;
  color: #999;
  padding: 20rpx 0;
  display: flex;
  align-items: center;
}
.myTeam .content-list-bottom-agency_wrap text {
  flex: 1;
  display: flex;
  align-items: center;
}
.myTeam .content-list-bottom-agency_wrap text:nth-child(1) {
  text-align: left;
}
.myTeam .content .no_data1 {
  font-size: 30rpx;
  color: #999;
  text-align: center;
  padding: 20rpx 0;
  width: 100%;
}
.myTeam .content .no_data2 {
  font-size: 30rpx;
  color: #666;
  text-align: center;
  margin: 100rpx auto;
}
.myTeam .van-tabs__nav {
  width: 120%;
  -webkit-transform: translateX(15px);
  transform: translateX(15px);
}
.myTeam .van-tabs__line {
  -webkit-transform: translateX(15px);
  transform: translateX(15px);
}
.myTeam .tab-active-class {
  -webkit-transform: translateX(15px);
  transform: translateX(15px);
}
.myTeam .van-ellipsis {
  flex-basis: 30%;
  width: 60%;
  text-align: center;
  overflow: visible;
}
.myTeam .van-tab__title {
  -webkit-transform: translateX(15px);
  transform: translateX(15px);
}

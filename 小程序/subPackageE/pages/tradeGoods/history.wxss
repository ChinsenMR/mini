page {
  background: #f7f7f7;
}

page .tradeGoods-wrap .history-container {
  border-radius: 16rpx;
  background-color: #fff;
}

page .tradeGoods-wrap .history-container .history-bar {
  border-bottom: 2rpx solid #EEEEEE;
}

page .tradeGoods-wrap .history-container .history-bar-trade-code {
  flex: 1;
}

page .tradeGoods-wrap .history-container .history-bar-trade-date {
  color: #999999;
}

page .tradeGoods-wrap .history-container .goods-bar-cover {
  width: 164rpx;
  height: 164rpx;
  margin-right: 17rpx;
}

page .tradeGoods-wrap .history-container .goods-bar-detail {
  width: 470rpx;
}

page .tradeGoods-wrap .history-container .goods-bar-detail-name {
  font-weight: 400;
  color: #333333;
}

page .tradeGoods-wrap .history-container .goods-bar-detail-sku {
  display: inline-block;
  padding: 4rpx 8rpx;
  background: #eeeeee;
  color: #666666;
}

page .tradeGoods-wrap .history-container .goods-bar-detail-price {
  color: #333333;
}

page .tradeGoods-wrap .history-container .goods-bar-count {
  align-items: flex-end;
  color: #999999;
}

/* pages/newAddress/newAddress.wxss */

page {
  width: 100%;
  height: 100%;
  background: #f7f7f7;
  overflow: hidden;
}
.fjh{
  height: 1px;
  
}
.box {
  width: 100%;
  height: 100%;
  overflow: auto;
  /* padding: 30rpx 30rpx; */
  box-sizing: border-box;
}

.fromeBox {
  width: 100%;
  box-sizing: border-box;
  border-radius: 10rpx;
  background: #fff;
}

.fromtop {
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: rgba(244, 244, 244, 1);
}

.shibie {
  width: 100%;
  display: flex;
  position: relative;
  align-items: center;
  box-sizing: border-box;
  font-size: 26rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  padding: 30rpx 30rpx;
  color: rgba(177, 177, 177, 1);
}
.shibietext {
  height: 100rpx;
  width: 80%;
  /* background: #B1B1B1; */
  font-size: 26rpx;
  color: #B1B1B1;
}
.shibiebtn {
  height: 40rpx;
  color: #FFF;
  padding: 5rpx 10rpx;
  border-radius: 10rpx;
  position: absolute;
  right: 25rpx;
  background: #CCCCCC;
}

.fromeone {
  display: flex;
  align-items: center;
  height: 100rpx;
  padding: 0rpx 26rpx;
  border-bottom: 1px solid #e5e5e5;
}

.username {
  display: flex;
  align-items: center;
  color: #333;
  width: 150rpx;
  font-size: 30rpx;
}

.ipt {
  height: 100%;
  flex: 1;
  font-size: 26rpx;
  color: #999;
  text-align: right;
  /* background: yellow; */
}
.arrow-right {
  /* line-height: 26rpx; */
  width: 26rpx;
}

.submintBtn {
  /* margin-top: 100rpx; */
  width: 90%;
  position: absolute;
  bottom: 100rpx;
  margin-left: 5%;
  /* background: #f44; */
  border-radius: 50rpx;
  padding: 20rpx 0rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
  background: #CCCCCC;
}
.defluterAddressBox{
  display: flex;
  align-items: center;
  height: 100rpx;
  background: #FFFFFF;
  margin-top: 20rpx;
  justify-content: space-between;
  padding: 0rpx 26rpx;
  
}
.defaultetip {
  display: flex;
  align-items: center;
  color: #333;
  font-size: 30rpx;
}
.fromeBox .fromeone:last-child{
  border: none;
}
.placeHolder{
  flex: 1;
  text-align: right;
  font-size:26rpx;
  color:#999;
}
.fromeone text{
    flex: 1;
    text-align: right;
    font-size:26rpx;
    color:#999;
}
.showSubmintBtn {
  background: #FF6666;
}
/* pages/receivingAddress/receivingAddress.wxss */
page{
  width: 100%;
  height: 100%;
  background: #F7F7f7;
}
view{
    box-sizing: border-box;
}
.item-box{
    /* width: 700rpx; */
    width: 100%;
    margin: 0 auto;
    padding:40rpx 40rpx;
    background: #F7F7f7;

}
.items{
    width: 100%;
    box-sizing: border-box;
    /* background: #FFF; */
    /* display: flex;
    flex-direction: column; */
}
.item{
    position: relative;
    margin-bottom: 20rpx;
    border-radius: 20rpx;
    border-top: 2rpx solid #eee;

    /* height: 120rpx; */
    
    /* overflow: hidden; */
}
.item:last-child{
    border-bottom: 2rpx solid #eee;
}
.inner{
    position: absolute;
    top:0;
}
.txt{
    width: 100%;
    position: relative;
    box-sizing: border-box;
    transition: left 0.2s ease-in-out;
    text-overflow: ellipsis;
    background: #FFF;
}
.fu{
  position: relative;
  z-index: 99;
    box-sizing: border-box;
     transition: transform 0.2s ease-in-out;
    background: #FFF;
    padding: 36rpx 36rpx;
}
.inner.del{
    width: 180rpx;
    background: red;
    text-align: center;
    z-index: 4;
    right: 0;
    color: #fff;
    height:100%;
    display: flex;
    font-size: 24rpx;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.del image{
  width: 50rpx;
  height: 50rpx;
}
.item-icon{
    width: 64rpx;
    height: 120rpx;
    vertical-align: middle;
    margin-right: 16rpx;
    background: #FFF;
}
.useranameBox{
  display: flex;

  align-items: center;
}
.userName{
  font-size: 28rpx;
  color: #333333;
}
.userPhone{
  font-size: 24rpx;
  color: #333333;
  padding: 0rpx 20rpx;
}
.iddefault{
  font-size: 20rpx;
  color: #FFF;
  padding: 5rpx 10rpx;
  background: #479EFF;
  border-radius: 40rpx;
}
.addressBox{
  justify-content: space-between;
  background: red;
position: relative;
  display: flex;
  align-items: center;
  width: 100%;
  background: #FFF;
  margin-bottom: 20rpx;
}
.userAddress{
  font-size:22rpx;
  color:#666666;
  width:80%;
  font-family:"PingFang-SC-Medium";
  overflow: hidden;
  word-wrap:break-word;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;


}
.imgbox {
  width: 40rpx;
  height: 40rpx;
  
}
.addBox{
  width: 100%;
  background: #FFF;
  display: flex;
  height: 200rpx;
  border-radius: 10rpx;
  align-items: center;
  justify-content: center;
}
.addBox image{
  width: 100rpx;
  height: 100rpx;
}

/**直播提示弹窗*/
.live_auto {
  height: 50rpx;
  position: absolute;
  top: 10%;
  right: 0;
  border: 1rpx 0 1rpx 1rpx solid #ffbbc9;
  box-sizing: border-box;
  border-radius: 25rpx 0 0 25rpx;
  background: rgba(255, 94, 125, 0.3);
  padding-left: 5rpx;
  width: 200rpx;
}

.live_tip {
  width: 200rpx;
  height: 50rpx;
  border-radius: 25rpx 0 0 25rpx;
  background: rgba(255, 94, 125, 0.3);
  display: flex;
  align-items: center;
}

.live_tip .live_img {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.live_text {
  font-size: 22rpx;
  color: #fff;
  width: 100rpx;
}

.wrap {
  width: 18px;
  height: 30rpx;
  margin: 10rpx auto 0;
  position: absolute;
  bottom: 4px;
  right: 45rpx;
  display: inline-block;
  vertical-align: baseline;
  font-size: 0;
  overflow: hidden;
}

.wrap .m1,
.m2,
.m3 {
  background-color: #fff;
  display: inline-block;
  width: 10rpx;
  height: 100rpx;
  border-radius: 5rpx;
}

.m1 {
  -webkit-animation: .8s .1s living linear infinite backwards normal;
  animation: .8s .1s living linear infinite backwards normal;
  -webkit-animation-delay: -1.1s;
}

.m2 {
  -webkit-animation: .8s .3s living linear infinite backwards normal;
  animation: .8s .3s living linear infinite backwards normal;
  -webkit-animation-delay: -1.3s;
}

.m3 {
  -webkit-animation: .8s .6s living linear infinite backwards normal;
  animation: .8s .6s living linear infinite backwards normal;
  -webkit-animation-delay: -1.6s;
}

@-webkit-keyframes living {
  0% {
    -webkit-transform: scaleY(.6);
    transform: scaleY(.6);
    -webkit-transform-origin: 0 120rpx;
    transform-origin: 0 120rpx
  }

  50% {
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
    -webkit-transform-origin: 0 120rpx;
    transform-origin: 0 120rpx
  }


  100% {
    -webkit-transform: scaleY(.6);
    transform: scaleY(.6);
    -webkit-transform-origin: 0 120rpx;
    transform-origin: 0 120rpx
  }
}

@keyframes living {
  0% {
    -webkit-transform: scaleY(.6);
    -ms-transform: scaleY(.6);
    transform: scaleY(.6);
    -webkit-transform-origin: 0 120rpx;
    -ms-transform-origin: 0 120rpx;
    transform-origin: 0 120rpx
  }

  50% {
    -webkit-transform: scaleY(1);
    -ms-transform: scaleY(1);
    transform: scaleY(1);
    -webkit-transform-origin: 0 120rpx;
    -ms-transform-origin: 0 120rpx;
    transform-origin: 0 120rpx
  }

  100% {
    -webkit-transform: scaleY(.6);
    -ms-transform: scaleY(.6);
    transform: scaleY(.6);
    -webkit-transform-origin: 0 120rpx;
    -ms-transform-origin: 0 120rpx;
    transform-origin: 0 120rpx
  }
}


@keyframes slideInUp {
  0% {
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
    visibility: visible;
  }
  100% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
/* 向中间消失 */
.slideInUp {
  -webkit-animation-name: slideInUp;
  animation-name: slideInUp;
}
@keyframes slideOutDown {
  0% {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  100% {
    visibility: hidden;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
  }
}
/* 向外消失 */
.slideOutDown {
  -webkit-animation-name: slideOutDown;
  animation-name: slideOutDown;
}
@keyframes fadeOut {
  0% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}
.fadeOut {
  -webkit-animation-name: fadeOut;
  animation-name: fadeOut;
}
@keyframes fadeIn {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
.fadeIn {
  -webkit-animation-name: fadeIn;
  animation-name: fadeIn;
}
@keyframes fadeInLeft {
  from {
    -webkit-transform: translate3d(-100%, 0, 0);
    transform: translate3d(-100%, 0, 0);
  }
  to {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}
.fadeInLeft {
  -webkit-animation-name: fadeInLeft;
  animation-name: fadeInLeft;
}
@keyframes fadeInRight {
  from {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
  to {
    -webkit-transform: translate3d(-100%, 0, 0);
    transform: translate3d(-100%, 0, 0);
  }
}
.fadeInRight {
  -webkit-animation-name: fadeInRight;
  animation-name: fadeInRight;
}
.layout {
  background-color: #f7f7f7;
  padding-top: 128rpx;
  height: 100%;
  box-sizing: border-box;
}
.layout .layout_nav {
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 30rpx 20rpx;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  background-color: #fff;
}
.layout .layout_nav .nav {
  width: 190rpx;
  height: 49rpx;
  border-radius: 25rpx;
  border: 1rpx solid #ff4343;
  font-size: 24rpx;
  color: #ff4343;
  text-align: center;
  line-height: 49rpx;
  margin-right: 20rpx;
  max-width: 220rpx;
}
.layout .layout_nav .active {
  color: #fff;
  background-color: #ff4343;
}
.layout .goods-comment {
  background-color: #fff;
  padding: 30rpx 30rpx 15rpx 30rpx;
  margin-bottom: 20rpx;
}
.layout .goods-comment .userInfo .userInfoImg {
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.layout .goods-comment .userInfo .userInfo-inner {
  display: flex;
  flex-direction: column;
}
.layout .goods-comment .userInfo .userInfo-inner .userInfo-innerT .user_wrap .userName {
  color: #333;
  font-size: 26rpx;
}
.layout .goods-comment .userInfo .userInfo-inner .userInfo-innerT .buyTime {
  color: #999;
  font-size: 20rpx;
}
.layout .goods-comment .userInfo .userInfo-inner .userInfo-gradeStar image {
  width: 19rpx;
  margin-right: 9rpx;
}
.layout .goods-comment .commoentInfo {
  color: #666;
  font-size: 24rpx;
  margin-top: 20rpx;
}
.layout .goods-comment .commoentImg {
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.layout .goods-comment .commoentImg .video_wrap {
  width: 220rpx;
  height: 230rpx;
}
.layout .goods-comment .commoentImg .video_wrap .myVideo {
  width: 100%;
  height: 100%;
}
.layout .goods-comment .commoentImg image {
  width: 220rpx;
  height: 220rpx;
  border: 1rpx solid #eee;
  border-radius: 10rpx;
}
.layout .goods-comment .reply {
  font-size: 30rpx;
  color: #333;
  padding: 20rpx 0;
  border-top: 1rpx solid #eee;
}
.layout .goods-comment .reply text {
  font-size: 24rpx;
  color: #999;
}
.layout .nav_nodata {
  font-size: 28rpx;
  color: #999;
  text-align: center;
  margin: 100rpx auto;
}
.userComment {
  background-color: #f7f7f7;
}

/* 上传图片样式 */
.about-content {
  background-color: #fff;
  padding: 30rpx;
  border-top: 20rpx solid rgb(248, 248, 248);
}

.filed {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 30rpx;
}

.filed text {
  font-size: 24rpx;
  color: #999;
}

.image-wrap {

  display: flex;
  flex-wrap: wrap;
}

.image-wrap .item {
  position: relative;
  /* display: flex; */
  /* align-items: center; */
  /* justify-content: center; */
  width: 329rpx;
  height: 205rpx;
  border: 1px solid rgba(153, 153, 153, 1);
  border-radius: 16rpx;
  float: left;
  margin-right: 24rpx;
  margin-bottom: 70rpx;
}

.image-wrap .item .icon {
  display: block;
  margin: 60rpx auto 20rpx auto;
  width: 66rpx;
  height: 66rpx;
}

.image-wrap .item .text {
  position: absolute;
  font-size: 24rpx;
  font-family: PingFang SC;
  text-align: center;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  bottom: -50rpx;
  width: 100%;
  text-align: center;
}

.image-wrap .item:nth-child(even) {
  margin-right: 0;
}


.win_style {
  width: 100%;
  background-color: #fff;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}

.win_main {
  width: 100%;
  height: 900rpx;
  background: rgba(0, 0, 0, .6);
  position: relative;
}

.search_btn {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin-right: 15rpx;
  border: 1rpx solid #fff;
  padding: 5rpx;
  box-sizing: border-box;
}

.btn_img {
  width: 100%;
  height: 100%;
  background-color: #fff;
  border-radius: 50%;
}

.btn_wrap {
  position: absolute;
  left: 0;
  width: 100%;
  bottom: 0;
  font-size: 30rpx;
  color: #fff;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.zhezhao {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  /* background: rgba(244, 244, 244, .6); */
  z-index: 99;
}

.content-li image {
  width: 23rpx;
  margin-left: 10rpx;
}

.content-li input {
  font-size: 28rpx;
  color: #333;
  width: 352rpx;
  text-align: right;
}

.inputStyle {
  color: #999;
}

.btn {
  /* position: fixed;

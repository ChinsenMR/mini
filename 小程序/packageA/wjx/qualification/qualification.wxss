#qualification {
  width: 100%;
  height: 100%;
  background: #f2f2f2;
}
#qualification .header {
  background-color: #fff;
  padding: 0 20rpx;
  margin-bottom: 20rpx;
}
#qualification .header .info {
  padding: 10rpx 0;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #eee;
}
#qualification .header .info .info_t {
  flex: 1;
  font-size: 26rpx;
  color: #333;
}
#qualification .header .info input {
  flex: 4;
  font-size: 26rpx;
  color: #333;
}
#qualification .content {
  background: #fff;
  padding: 0 15rpx;
}
#qualification .content .code_wrap {
  display: flex;
  flex-direction: column;
  border-bottom: 1rpx solid #eee;
  padding-bottom: 15rpx;
}
#qualification .content .code_wrap .code_t {
  font-size: 30rpx;
  color: #333;
  font-weight: 600;
  padding-top: 10rpx;
}
#qualification .content .code_wrap .code_img {
  width: 80rpx;
  height: 80rpx;
}
#qualification .content .code_wrap .pictbox {
  width: 100%;
  display: flex;
  /* margin-top: 30rpx; */
  flex-wrap: wrap;
}
#qualification .content .code_wrap .pictbox .evalteimg {
  width: 150rpx;
  height: 150rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
  position: relative;
  border: 1px solid #c8c8c8;
}
#qualification .content .code_wrap .pictbox .tu {
  width: 100%;
  height: 100%;
}
#qualification .content .code_wrap .pictbox .close {
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: -14rpx;
  right: -13rpx;
}
#qualification .content .code_wrap .takepic {
  margin-top: 20rpx;
  width: 150rpx;
  height: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  border: 1px solid #c8c8c8;
}
#qualification .content .code_wrap .takepic .pict {
  width: 62rpx;
  height: 50rpx;
}

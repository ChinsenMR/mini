#qualification .btn_wrap {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 100rpx 0 30rpx 0;
}
#qualification .btn_wrap .btn {
  width: 90%;
  height: 30rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
  line-height: 30rpx;
  background-color: red;
  border-radius: 10rpx;
  padding: 15rpx 0;
}
#qualification .win {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background-color: #fff;
  z-index: 101;
  width: 80%;
  height: 500rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  border-radius: 10rpx;
}
#qualification .win .win_img {
  width: 150rpx;
  height: 150rpx;
  margin-top: 20rpx;
}
#qualification .win .win_tt {
  margin-bottom: 16rpx;
}
#qualification .win .win_btn {
  width: 60%;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  color: #fff;
  background: #169bd5;
  margin-bottom: 40rpx;
  border-radius: 10rpx;
}
#qualification .win .add_active {
  background-color: #fff;
  color: #333;
  border: 1rpx solid #eee;
}
#qualification .win .add_win {
  margin-bottom: 15rpx;
}
#qualification .zhezhao {
  width: 100%;

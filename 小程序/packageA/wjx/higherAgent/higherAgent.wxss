#higherAgent {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
}
#higherAgent .header {
  background-color: #fff;
  padding: 0 20rpx;
}
#higherAgent .header .hiti {
  padding: 20rpx 0 10rpx 0;
}
#higherAgent .header .info {
  padding: 10rpx 0;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #eee;
}
#higherAgent .header .info .title {
  flex: 1;
}
#higherAgent .header .info input {
  flex: 4;
}
#higherAgent .content {
  background: #fff;
  padding: 0 20rpx;
  margin-top: 20rpx;
}
#higherAgent .content .code_wrap {
  display: flex;
  flex-direction: column;
  border-bottom: 1rpx solid #eee;
  padding-bottom: 20rpx;
}
#higherAgent .content .code_wrap .code_t {
  font-size: 26rpx;
  color: #333;
  font-weight: 600;
  padding: 10rpx 0;
}
#higherAgent .content .code_wrap .code_img {
  width: 150rpx;
  height: 150rpx;
}
#higherAgent .content .xg_agent {
  display: flex;
  flex-direction: column;
  padding-bottom: 20rpx;
}
#higherAgent .content .xg_agent .xg_zizhi {
  font-size: 26rpx;
  color: #333;
  font-weight: 600;
  padding: 10rpx 0;
}
#higherAgent .content .xg_agent .img_wrap {
  display: flex;
  align-items: center;
}
#higherAgent .content .xg_agent .img_wrap .one_img {
  width: 150rpx;
  height: 150rpx;
  margin-right: 20rpx;
}

.withdrawal_way {
  padding: 20rpx 0;
}
.withdrawal_way .withdrawal_way_wrap {
  font-size: 26rpx;
  color: #333;
  background-color: #fff;
  box-sizing: border-box;
  padding: 0 20rpx;
}
.withdrawal_way .withdrawal_way_wrap .withdrawal_wx {
  display: flex;
  align-items: center;
  width: 100%;
  height: 118rpx;
  border-bottom: 1rpx solid #f5f5f5;
}
.withdrawal_way .withdrawal_way_wrap .withdrawal_wx .withdrawal_img {
  width: 60rpx;
  height: 60rpx;
}
.withdrawal_way .withdrawal_way_wrap .withdrawal_wx .wx_wrap {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 10rpx;
  font-size: 28rpx;
  font-weight: 600;
}
.withdrawal_way .withdrawal_way_wrap .withdrawal_wx .test {
  color: #FF3333;
  font-size: 22rpx;
}
.withdrawal_way .withdrawal_way_wrap .withdrawal_wx:last-of-type {
  border-bottom: none;
}
.withdrawal_way .withdrawal_more {
  margin-left: auto;
  width: 30rpx;
  height: 28rpx;
  margin-right: 20rpx;
}

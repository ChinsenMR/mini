.header{
  background-color: #fff;
  height: 208rpx;
  padding: 0 30rpx;
  margin-bottom: 20rpx;
}

.header image{
  width: 145rpx;
  height: 145rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}

.h-btn{
  background-color: #FF3333;
  width: 186rpx;
  line-height: 47rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
  border-radius: 24rpx;
  padding: 0;
  margin: 0;
}

.h-tip{
  flex: 1;
  color: #999;
  font-size: 26rpx;
  margin-top: 5rpx;
}

.u-ul{
  margin-top: 20rpx;
}

.u-li{
  height: 88rpx;
  padding: 0 30rpx;
  color: #333;
  font-size: 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #f4f4f4;
}

.u-li input{
  flex: 1;
  text-align: right;
}

.btn{
  background-color: #FF3333;
  color: #fff;
  font-size: 30rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius: 44rpx;
  width: 85%;
  margin: 50rpx auto;
}
.drop_out{
  font-size: 30rpx;
  color:#999;
  text-align: center;
  margin: 0 auto;
  padding: 20rpx 0;
}
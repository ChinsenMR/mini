.couponbox {
  padding: 30rpx;
  color: white;
}
.couponbox .defalut {
  display: flex;
  align-items: center;
  position: relative;
}
.couponbox .defalut .coul {
  width: 190rpx;
  max-width: 220rpx;
  margin-right: 30rpx;
}
.couponbox .defalut .coul .limt {
  display: flex;
  flex-direction: column;
  justify-content: center;
  height: 85%;
  align-items: center;
}
.couponbox .defalut .coul .limt .limt_price .dol {
  font-size: 33rpx;
}
.couponbox .defalut .coul .limt .limt_price .num {
  font-size: 70rpx;
}
.couponbox .defalut .coul .limt .man {
  font-size: 22rpx;
}
.couponbox .defalut .coum {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.couponbox .defalut .coum .new {
  font-size: 32rpx;
}
.couponbox .defalut .coum .tiem {
  font-size: 22rpx;
  margin-top: 19rpx;
  width: 210rpx;
}
.couponbox .defalut .coumr {
  width: 98rpx;
  height: 44rpx;
  background: #ffffff;
  border-radius: 22rpx;
  color: #fc4345;
  font-size: 26rpx;
  text-align: center;
  line-height: 44rpx;
  position: absolute;
  top: 36%;
  right: 30rpx;
}
.couponbox .defalut .emity {
  width: 98rpx;
}
.couponbox .coudet {
  width: 690rpx;
  height: 160rpx;
  background-image: url("http://hmqy.oss-cn-hangzhou.aliyuncs.com/hmeshop_jxy/images/pic_coupon_1@2x.png");
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.couponbox .coudet2 {
  width: 690rpx;
  height: 160rpx;
  background-image: url("http://hmqy.oss-cn-hangzhou.aliyuncs.com/hmeshop_jxy/images/pic_coupon_2@2x.png");
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

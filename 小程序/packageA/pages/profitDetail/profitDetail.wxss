.profitDetail {
  border-top: 1rpx solid #eee;
}
.profitDetail .content-list {
  background-color: #fff;
  margin-bottom: 20rpx;
}
.profitDetail .content-list-title {
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 20rpx;
  border-bottom: 1rpx solid #eee;
}
.profitDetail .content-list-title-left {
  font-size: 30rpx;
  color: #000000;
}
.profitDetail .content-list-title-right {
  display: flex;
  align-items: center;
  justify-content: center;
}
.profitDetail .content-list-title-right text {
  font-size: 30rpx;
  color: var(--theme-red);
}
.profitDetail .content-list-title-right image {
  width: 13rpx;
  height: 25rpx;
  margin-left: 10rpx;
}
.profitDetail .content-list-list_wrap {
  box-sizing: border-box;
  padding: 20rpx;
}
.profitDetail .content-list-list_wrap-main text {
  font-size: 24rpx;
  color: #666666;
}
.profitDetail .content-list-list_wrap-main text:last-child {
  color: #000000;
  margin-left: 10rpx;
}
.profitDetail .no-data {
  font-size: 30rpx;
  color: #999;
  text-align: center;
  padding: 20rpx 0 40rpx 0;
}
.profitDetail .no_data {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 200rpx auto;
}
.profitDetail .no_data image {
  width: 493rpx;
  height: 238rpx;
  margin-bottom: 30rpx;
}
.profitDetail .no_data text {
  font-size: 30rpx;
  color: #999999;
}

.header {
  background-color: #fff;
  height: 208rpx;
  padding: 0 30rpx;
  margin-bottom: 20rpx;
}
.status-wrap{
  width: 100%;
  color: #fff;
  height: 138rpx;
}
.header image {
  width: 145rpx;
  height: 145rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}

.h-btn {
  background-color: #ff3333;
  width: 186rpx;
  line-height: 47rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
  border-radius: 24rpx;
  padding: 0;
  margin: 0;
}

.h-tip {
  flex: 1;
  color: #999;
  font-size: 26rpx;
  margin-top: 5rpx;
}

.u-ul {
  margin-top: 20rpx;
}

.wrap-container {
  height: 88rpx;
  padding: 0 30rpx;
  color: #333;
  font-size: 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #f4f4f4;
}
label {
  width: 200rpx;
  font-size: 30rpx;
  /* font-family:PingFang SC; */
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}
.wrap-container input {
  flex: 1;
  font-size: 28rpx;
  height: 70rpx;
  text-align: left;
  color: rgba(153, 153, 153, 1);
}
.input-placeholder{
  font-size: 28rpx;
}

.btn {
  background-color: #ff3333;
  color: #fff;
  font-size: 30rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius: 44rpx;
  margin: 100rpx 30rpx;
}

.about-content {
  background-color: #fff;
  padding: 30rpx;
  border-top: 20rpx solid rgb(248, 248, 248);
}
.filed {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 30rpx;
}
.filed text {
  font-size: 24rpx;
  color: #999;
}
.image-wrap {

  display: flex;
  flex-wrap: wrap;
}
.image-wrap .item {
  position: relative;
  /* display: flex; */
  /* align-items: center; */
  /* justify-content: center; */
  width: 329rpx;
  height: 205rpx;
  border: 1px solid rgba(153, 153, 153, 1);
  border-radius: 16rpx;
  float: left;
  margin-right: 24rpx;
  margin-bottom: 70rpx;
}
.image-wrap .item .icon {
  display: block;
  margin: 60rpx auto 20rpx auto;
  width: 66rpx;
  height: 66rpx;
}
.image-wrap .item .text {
  position: absolute;
  font-size: 24rpx;
  font-family: PingFang SC;
  text-align: center;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  bottom: -50rpx;
  width: 100%;
  text-align: center;
}
.image-wrap .item:nth-child(even) {
  margin-right: 0;
}
.submit-btn {
  margin-left: 30rpx;
  margin-bottom: 30rpx;
  width: 690rpx;
  height: 88rpx;
  /* background: rgba(204, 204, 204, 1); */
  background: rgba(255, 68, 68, 1);
  border-radius: 44rpx;
  font-size: 30rpx;
  color: #fff;
}





/* 提现协议弹窗 */
.agree-dialog {
  position: fixed;
  overflow-y: scroll;
  -webkit-overflow-scrolling : touch; /* liuhx:可以把这整行注释掉对比差别 */
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  /* background-color: #fff; */
  z-index: 10999;
  border: 1rpx solid #eee;
  background-color: #fff;
  padding: 30rpx;

}

.win_style {
  width: 100%;
  color: #fff;
  background: rgba(255, 68, 68, 1);
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}
.search_title{
  color: rgba(255, 68, 68, 1);
}
.agree-dialog scroll-view{
  height: 86%;
}


.wrap-container{
  width: 100%;
}
.wxParse-p image{
  width: 100%!important;
}
.arrow-icon{
  width: 50rpx;
}
.picker-item{
  width: 500rpx;
}

.win_main {
  width: 100%;
  height: 900rpx;
  background: #fff;
  position: relative;
}

.search_btn {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin-right: 15rpx;
  border: 1rpx solid rgba(255, 68, 68, 1);
  padding: 5rpx;
  box-sizing: border-box;
}

.btn_img {
  width: 100%;
  height: 100%;
  background-color: rgba(255, 68, 68, 1);
  border-radius: 50%;
}

.btn_wrap {
  
  font-size: 30rpx;
  color: #fff;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.zhezhao {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  /* background: rgba(244, 244, 244, .6); */
  z-index: 99;
}
.filter-wrap{
  z-index: 1000;
}

.agree {
  margin-bottom: 60rpx;
  font-size: 24rpx;
  text-align: center;
  color: #999;
}

.agree text {
  color: #3195d3;
}